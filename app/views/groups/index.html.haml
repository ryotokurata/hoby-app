.wrapper
  = render "header"
  .main
    .content
      .hobbys
        - @groups.each do |group|
          .hobby-box
            =link_to group_messages_path(group.id) do
              .hobby-image
                =image_tag group.image.url,size: "200x200"
              .hobby-content
                = group.name
  .footer
    
